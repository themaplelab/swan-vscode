{
	"name": "swan",
	"displayName": "SWAN",
	"description": "VSCode integration for SWAN - Swift static analysis",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dataflow"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.dataflow",
				"title": "Generate Data Flow"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "taintAnalysisContainer",
					"title": "Taint Analysis",
					"icon": "./resources/icons/viewIcon.png"
				}
			]
		},
		"views": {
			"taintAnalysisContainer": [
				{
					"id": "taintAnalysisPanel",
					"name": "Paths"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
